    <i-tab key="tab1" title="待确认"></i-tab>
    <i-tab key="tab2" title="待我解决"></i-tab>
  </i-tabs>
  <scroll-view class="centerView">
    <view style="margin-top:10px">
      <i-cell-group>
        <i-cell bindtap="toDetail" title="打印机问题" value="2018.10.8 10:00">
          <i-icon type="createtask" size="20" slot="icon" />
        </i-cell>
        <i-cell bindtap="toDetail" title="导航台问题" value="2018.10.8 09:00">
          <i-icon type="createtask" size="20" slot="icon" />
        </i-cell>
        <i-cell bindtap="toDetail" title="电脑问题" value="2018.10.5 18:00">
          <i-icon type="createtask" size="20" slot="icon" />
        </i-cell>
      </i-cell-group>
    </view>
  </scroll-view>

</view>