.centerView {
  text-align: center;
  border-radius: 20px;
  width: 300px;
  height: 350px;
  position: absolute;
  left: 50%;
  top: 38%;
  transform: translate(-50%, -50%);
}

/*分割线样式*/

.divLine {
  background: #e9eaec;
  width: 100%;
  height: 5rpx;
  margin-top: 15px;
}

.footer {
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 50px;
  line-height: 50px;
}
